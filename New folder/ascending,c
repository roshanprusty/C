#include <stdio.h>

int main()
{

    int a, b, c;

    printf("enter the first nos");
    scanf("%d", &a);

    printf("enter the second nos");
    scanf("%d", &b);

    printf("enter the third nos");
    scanf("%d", &c);

    if (a > b > c)
    {
        printf("%d<%d<%d", a, b, c);
    }
    else if (b > a > c)
    {
        printf("%d>%d>%d", b, a, c);
    }

    else if (c > a > b)
    {
        printf("%d>%d>%d", c, a, b);
    }

    else
    {
        printf("sir serially do");
    }

    return 0;
}
